{
    "name": "kirkaracha/laravel-propublica-congress-api",
    "description": "Laravel package for consuming the ProPublica Congress API",
    "authors": [
        {
            "name": "Kirk Franklin",
            "email": "kirk@magneato.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^5.5"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kirkaracha\\PropublicaCongressApi\\PropublicaCongressApiServiceProvider"
            ],
            "aliases": {
                "PropublicaCongressApi": "Kirkaracha\\PropublicaCongressApi\\PropublicaCongressApiFacade"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kirkaracha\\PropublicaCongressApi\\Tests\\": "tests"
        }
    }
}
